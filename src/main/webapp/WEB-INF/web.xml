<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EmpHomeServlet</servlet-name>
    <servlet-class>EmpHomeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EmpResolvedServlet</servlet-name>
    <servlet-class>EmpResolvedServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EmpReceiptServlet</servlet-name>
    <servlet-class>EmpReceiptServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EmpReimbursementServlet</servlet-name>
    <servlet-class>EmpReimbursementServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>EmpUpdateServlet</servlet-name>
    <servlet-class>EmpUpdateServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ManHomeServlet</servlet-name>
    <servlet-class>ManHomeServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ManInfoServlet</servlet-name>
    <servlet-class>ManInfoServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ManResolvedServlet</servlet-name>
    <servlet-class>ManResolvedServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ManAllEmpsServlet</servlet-name>
    <servlet-class>ManAllEmpsServlet</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>ManUpdateServlet</servlet-name>
    <servlet-class>ManUpdateServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EmpHomeServlet</servlet-name>
    <url-pattern>/empHome</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EmpResolvedServlet</servlet-name>
    <url-pattern>/empResolved</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EmpReceiptServlet</servlet-name>
    <url-pattern>/empReceipt</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EmpReimbursementServlet</servlet-name>
    <url-pattern>/empReimbursement</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>EmpUpdateServlet</servlet-name>
    <url-pattern>/empUpdate</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ManHomeServlet</servlet-name>
    <url-pattern>/manHome</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ManInfoServlet</servlet-name>
    <url-pattern>/manInfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ManAllEmpsServlet</servlet-name>
    <url-pattern>/manAllEmps</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ManResolvedServlet</servlet-name>
    <url-pattern>/manResolved</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ManUpdateServlet</servlet-name>
    <url-pattern>/manUpdate</url-pattern>
  </servlet-mapping>
</web-app>
